<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.arslan.qrcode.ShowData"
    android:background="@drawable/login_background">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Numero Inventario: " />

    <TextView
        android:id="@+id/textView4"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="30dp"
        android:text="Nome cespite: " />

    <TextView
        android:id="@+id/textView5"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="29dp"
        android:text="Foto:" />

    <TextView
        android:id="@+id/textView6"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="32dp"
        android:text="Data di catalogazione:" />

    <TextView
        android:id="@+id/textView7"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="54dp"
        android:text="Descrizione cespite:" />

    <TextView
        android:id="@+id/textView9"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="69dp"
        android:text="Aula:" />

    <TextView
        android:id="@+id/textView11"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:text="Categoria:"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView13"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:text="Catalogatore:"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/Numinventario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/Datacat"
        android:text="TextView"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/Numinventario"
        android:layout_alignTop="@+id/textView4"
        android:text="TextView"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Foto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/Nome"
        android:text="TextView"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Datacat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_marginStart="39dp"
        android:layout_toEndOf="@+id/textView6"
        android:text="TextView"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Descrizione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignStart="@+id/Datacat"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/NumeroAula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_above="@+id/textView11"
        android:layout_toEndOf="@+id/textView5"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Categoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_above="@+id/textView13"
        android:layout_toEndOf="@+id/textView11"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Nome_user_registratore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignEnd="@+id/textView3"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/textView25"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo:"
        android:layout_alignTop="@+id/NumeroAula"
        android:layout_alignStart="@+id/textView26" />

    <TextView
        android:id="@+id/textView26"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descrizione:"
        android:layout_alignTop="@+id/Categoria"
        android:layout_toStartOf="@+id/Descrizione_categoria" />

    <TextView
        android:id="@+id/textView27"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ruolo:"
        android:layout_alignBaseline="@+id/Nome_user_registratore"
        android:layout_alignBottom="@+id/Nome_user_registratore"
        android:layout_alignStart="@+id/textView26" />

    <TextView
        android:id="@+id/TipologiaAula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBottom="@+id/textView25"
        android:layout_alignEnd="@+id/textView26"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Descrizione_categoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBaseline="@+id/textView26"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignParentEnd="true"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

    <TextView
        android:id="@+id/Ruolo_user_registratore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBottom="@+id/textView27"
        android:layout_toEndOf="@+id/textView27"
        android:textStyle="italic"
        android:textColor="@android:color/white"/>

</RelativeLayout>
